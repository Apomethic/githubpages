		s1.addProduct(p1);
		s1.addProduct(p2);
		s1.addProduct(p3);

		
		c1.buy(p1,s1);		
		c1.buy(p1,s1);
		c1.buy(p1,s1);
		c1.buy(p1,s1);		
		c1.buy(p1,s1);
		c1.buy(p1,s1);
		c1.buy(p3,s1);
		c1.buy(p2,s1);
